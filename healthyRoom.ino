String usermail="";
String userpassword="";
String sendToMail="";
String lowTemp = "";
String highTemp = "";
String lowHumidity = "";
String highHumidity = "";

String usermail2="";
String userpassword2="";
String sendToMail2="";
String lowVOC = "";
String highVOC = "";
String lowCO2 = "";
String highCO2 = "";

EMailSender::EMailMessage message;

//Environment Humidity and Temperature Sensor as SHT shell
SHT3X sht30(0x45);

void setup()
{
  Serial.begin(115200);
  
  //connecting to Wifi
  WiFi.begin(ssid, password) ; // make the WiFi connection
  Serial.println("Start connecting.") ;
  while (WiFi.status()  != WL_CONNECTED) {
    delay(500);
    Serial.print(".") ;
  }

  Serial.print("Connected to ") ;
  Serial.print(ssid) ;
  Serial.print(". IP address: ") ;
  Serial.println(WiFi.localIP( ) ) ;

  //Request controls on web server communication
  server.on("/", HTTP_GET, [](AsyncWebServerRequest * request) {
    request->send_P(200, "text/html", webPage);
  });

  //first web-page
  server.on("/get", HTTP_GET, [] (AsyncWebServerRequest * request) {
    if (request->hasParam(PARAM_INPUT_1)) {
      usermail = request->getParam(PARAM_INPUT_1)->value();
    }
    if (request->hasParam(PARAM_INPUT_2)) {
      userpassword = request->getParam(PARAM_INPUT_2)->value();
    }
    if (request->hasParam(PARAM_INPUT_3)) {
      sendToMail = request->getParam(PARAM_INPUT_3)->value();
    }
    if (request->hasParam(PARAM_INPUT_4)) {
      lowTemp = request->getParam(PARAM_INPUT_4)->value();
    }
    if (request->hasParam(PARAM_INPUT_5)) {
      highTemp = request->getParam(PARAM_INPUT_5)->value();
    }
    if (request->hasParam(PARAM_INPUT_6)) {
      lowHumidity = request->getParam(PARAM_INPUT_6)->value();
    }
    if (request->hasParam(PARAM_INPUT_7)) {
      highHumidity = request->getParam(PARAM_INPUT_7)->value();
    }
    request->send_P(200, "text/html", webPage2);
  });

  //second web-page
  server.on("/get2", HTTP_GET, [] (AsyncWebServerRequest * request) {
    if (request->hasParam(PARAM_INPUT_11)) {
      usermail2 = request->getParam(PARAM_INPUT_11)->value();
    }
    if (request->hasParam(PARAM_INPUT_12)) {
      userpassword2 = request->getParam(PARAM_INPUT_12)->value();
    }
    if (request->hasParam(PARAM_INPUT_13)) {
      sendToMail2 = request->getParam(PARAM_INPUT_13)->value();
    }
    if (request->hasParam(PARAM_INPUT_15)) {
      highCO2 = request->getParam(PARAM_INPUT_15)->value();
    }
    if (request->hasParam(PARAM_INPUT_17)) {
      highVOC = request->getParam(PARAM_INPUT_17)->value();
    }
    request->send(200, "text/text","YOU CAN CLOSE THIS PAGE! IT IS INITIALIZED!!!");
  });

  server.onNotFound(notFound);
  //initiating server
  server.begin( ) ;// start the server for WiFi input
  Serial.println("HTTP server started");
  
 

  while(usermail == "" || userpassword == "" || sendToMail == "" || lowTemp == "" || highTemp  == ""|| lowHumidity  == "" || highHumidity  == "" || usermail2 == "" || userpassword2 == "" || sendToMail2 == "" || highCO2  == "" || highVOC  == "" ){
    Serial.println("waiting for credentials");
    delay(10000);
  }

   if(!ccs.begin()){
    Serial.println("Failed to start sensor! Please check your wiring.");
    while(1);
  }
   while(!ccs.available()){
      delay(10);
  }
}
 
void loop() {
  EMailSender emailSend(usermail.c_str(), userpassword.c_str()); //Environmental Humidity and Temperature Postman
  EMailSender emailSend2(usermail2.c_str(), userpassword2.c_str()); //Air Quality Postman


  /*
      MAIL LAYOUT: 
      message.subject <- mail subject
      message.message <- your mail content (add different types carefully with "+=" if it is needed)
      EMailSender::Response resp = emailSend.send(sendToMail.c_str(), message); <- sends your mail to sendToMail person with your favourite postman 
  */
  
  while(true){
  delay(30000);
  //ENVIRONMENTAL FEEDBACKS TO SECURITY
  if(sht30.get()==0){
    Serial.print("Temperature in Celsius : ");
    Serial.println(sht30.cTemp);
    Serial.print("Temperature in Humidity : ");
    Serial.println(sht30.humidity);
    if(sht30.cTemp < lowTemp.toInt())
    {
      message.subject = "LOW TEMPERATURE"; 
      message.message = "Temperature is LOW: ";
      message.message += sht30.cTemp;
      message.message += "! Place need to be keep warmer!!!";
      EMailSender::Response resp = emailSend.send(sendToMail.c_str(), message);
 
      Serial.println("Sending status: ");
      Serial.println(resp.code);
      Serial.println(resp.desc);
      Serial.println(resp.status);
    }
    else if(sht30.cTemp > highTemp.toInt())
    {
      message.subject = "HIGH TEMPERATURE";
      message.message = "Temperature is HIGH: " ;
      message.message += sht30.cTemp ;
      message.message += "! Place need to be keep colder!!!";
      EMailSender::Response resp = emailSend.send(sendToMail.c_str(), message);
 
      Serial.println("Sending status: ");
      Serial.println(resp.code);
      Serial.println(resp.desc);
      Serial.println(resp.status);
    }
   
    if(sht30.humidity < lowHumidity.toInt())
    {
      message.subject = "LOW HUMIDITY";
      message.message = "Humidity is LOW: ";
      message.message += sht30.humidity ;
      message.message += "! Place need to be more humid!!!";
      EMailSender::Response resp = emailSend.send(sendToMail.c_str(), message);
 
      Serial.println("Sending status: ");
      Serial.println(resp.code);
      Serial.println(resp.desc);
      Serial.println(resp.status);
    }
    else if(sht30.humidity > highHumidity.toInt())
    {
      message.subject = "HIGH HUMIDITY";
      message.message = "Humidity is HIGH: ";
      message.message += sht30.humidity;
      message.message += "! Place need to be keep drier!!!";
      EMailSender::Response resp = emailSend.send(sendToMail.c_str(), message);
 
      Serial.println("Sending status: ");
      Serial.println(resp.code);
      Serial.println(resp.desc);
      Serial.println(resp.status);
    }
  }
  else
  {
    Serial.println("Error on SHT!");
  }
  //AIR QUALITY FEEDBACKS TO SECURITY
  if(ccs.available()){
    if(!ccs.readData()){
    Serial.print("CO2 level: ");
    Serial.println(ccs.geteCO2());
    Serial.print("VOC level: ");
    Serial.println(ccs.getTVOC());
    if(ccs.geteCO2() > highCO2.toInt())
    {
      message.subject = "HIGH CO2 LEVEL";
      message.message = "CO2 is HIGH: " ;
      message.message += ccs.geteCO2();
      message.message += "! Place need to be keep conditioned, more oxygen needed!!!";
      EMailSender::Response resp = emailSend2.send(sendToMail2.c_str(), message);
 
      Serial.println("Sending status: ");
      Serial.println(resp.code);
      Serial.println(resp.desc);
      Serial.println(resp.status);
    }
  
    if(ccs.getTVOC() > highVOC.toInt())
    {
      message.subject = "HIGH VOC";
      message.message = "VOC is HIGH: ";
      message.message += ccs.getTVOC();
      message.message += "! Place need to be conditioned VOC is dangerous!!!";
      EMailSender::Response resp = emailSend2.send(sendToMail2.c_str(), message);
 
      Serial.println("Sending status: ");
      Serial.println(resp.code);
      Serial.println(resp.desc);
      Serial.println(resp.status);
    }}
    else{
      Serial.println("ERROR");
      while(1);
    }
    
  }
  delay(30000);}
 
